<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LetsMeet" xmlns:system="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="LatoSubham">pack://application;,,,/Fonts/#Lato Light</FontFamily>
    <FontFamily x:Key="LatoBoldSubham">pack://application;,,,/Fonts/#Lato Bold</FontFamily>
    <FontFamily x:Key="FontAwesome">pack://application;,,,/Fonts/#FontAwesome</FontFamily>

    <system:Double x:Key="FontSizeSmaller">10</system:Double>
    <system:Double x:Key="FontSizeSmall">12</system:Double>
    <system:Double x:Key="FontSizeRegular">14</system:Double>
    <system:Double x:Key="FontSizeLarge">20</system:Double>
    <system:Double x:Key="FontSizeXLarge">24</system:Double>
    <system:Double x:Key="FontSizeXXLarge">30</system:Double>

    <system:String x:Key="FontAwesomeChatIcon">&#xf075;</system:String>
    <system:String x:Key="FontAwesomeContactsIcon">&#xf011;</system:String>
    <system:String x:Key="FontAwesomeSharedFilesIcon">&#xf1c5;</system:String>
    <system:String x:Key="FontAwesomeSettingsIcon">&#xf013;</system:String>
    <system:String x:Key="FontAwesomeEllipsesVerticalIcon">&#xf142;</system:String>
    <system:String x:Key="FontAwesomeSearchIcon">&#xf002;</system:String>

</ResourceDictionary>